{
  "buildCommand": "vite build",
  "outputDirectory": "./dist/public",
  "framework": null,
  "functions": {
    "server/index.ts": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/index.ts"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}